; Test scene with no UI elements - should not show any grey panels

(defscene3d clean-scene
  (camera
    (position 5 3 5)
    (target 0 0 0)
    (fov 60.0))
    
  (lighting
    (ambient 0.3 0.3 0.3)
    (directional
      (direction 1 1 1)
      (color 1.0 1.0 1.0)
      (intensity 1.0)))
  
  ; Just a simple cube - no UI elements defined
  (object test-cube cube
    (position 0 0 0)
    (scale 1 1 1)
    (material mesh-basic
      (color "#00FF00")
      (opacity 1.0)
      (side front))))